{
  "name": "tree-sitter-hledger",
  "version": "0.3.0",
  "description": "A parser for the hledger double-book accounting file format.",
  "repository": "https://github.com/yeldirium/tree-sitter-hledger",
  "license": "MIT",
  "author": {
    "name": "Hannes Leutloff",
    "email": "hannes.leutloff@yeldirium.de",
    "url": "https://yeldirium.de/"
  },
  "main": "bindings/node",
  "type": "module",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "hledger"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.2.1",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.1"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  }
}
