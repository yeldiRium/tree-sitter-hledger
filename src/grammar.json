{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "hledger",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_spaces1"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "SYMBOL",
              "name": "_newline"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "account_directive"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "account_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "account"
        },
        {
          "type": "SYMBOL",
          "name": "_spaces1"
        },
        {
          "type": "FIELD",
          "name": "account_name",
          "content": {
            "type": "SYMBOL",
            "name": "account_name"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "comment",
              "content": {
                "type": "SYMBOL",
                "name": "inline_comment"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "account_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "account_name_segment"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ":"
              },
              {
                "type": "SYMBOL",
                "name": "account_name_segment"
              }
            ]
          }
        }
      ]
    },
    "inline_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_spaces2"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ";"
            },
            {
              "type": "STRING",
              "value": "#"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_spaces1"
                },
                {
                  "type": "SYMBOL",
                  "name": "_comment_content"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "comment": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": ";"
              },
              {
                "type": "STRING",
                "value": "#"
              },
              {
                "type": "STRING",
                "value": "*"
              }
            ]
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "_spaces1"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_comment_content"
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "_comment_content": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_comment_word"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_spaces1"
                },
                {
                  "type": "SYMBOL",
                  "name": "_comment_word"
                }
              ]
            }
          }
        ]
      }
    },
    "inline_comment_with_tags": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_spaces2"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ";"
            },
            {
              "type": "STRING",
              "value": "#"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_spaces1"
                },
                {
                  "type": "SYMBOL",
                  "name": "_inline_comment_with_tags_content"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_inline_comment_with_tags_content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_comment_word"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "_spaces1"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "_inline_comment_with_tags_content"
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "tag"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "STRING",
                      "value": ","
                    },
                    {
                      "type": "SYMBOL",
                      "name": "_spaces1"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "_inline_comment_with_tags_content"
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        }
      ]
    },
    "tag": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "key",
            "content": {
              "type": "SYMBOL",
              "name": "tag_key"
            }
          },
          {
            "type": "FIELD",
            "name": "value",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "tag_value"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "account_name_segment": {
      "type": "PATTERN",
      "value": "[^\\(\\)\\[\\]: \\n]+( [^\\(\\)\\[\\]: \\n]+)*"
    },
    "_comment_word": {
      "type": "PATTERN",
      "value": "[^: \\n]+"
    },
    "tag_key": {
      "type": "PATTERN",
      "value": "[^:, \\n]+:"
    },
    "tag_value": {
      "type": "PATTERN",
      "value": "[^,\\n]+"
    },
    "_spaces1": {
      "type": "PATTERN",
      "value": " {1,}"
    },
    "_spaces2": {
      "type": "PATTERN",
      "value": " {2,}"
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\n"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}